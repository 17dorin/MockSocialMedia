@model List<Post>

@if(Model.Count != 0)
{
    foreach(Post p in Model)
    {

        <h3>@p.Content</h3>
        <h3>@p.Likes Likes</h3>
        <a asp-action="ConfirmDeletePost" asp-controller="Post" asp-route-postToDelete="@p.Id">Delete</a>
        <br/>
    }
}
else
{
    <h3>You do not have any posts</h3>
}
